<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <title>@ViewBag.Title | MPS Dispatch</title>
  @RenderSection("head", required: false)
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.7 -->
  <link href="~/assets/libs/bootstrap3/css/bootstrap.min.css" rel="stylesheet" />

  <!-- Font Awesome -->
  <link href="~/assets/libs/font-awesome/css/font-awesome.min.css" rel="stylesheet" />

  <!-- Ionicons -->
  <link href="~/assets/libs/ionicons/css/ionicons.min.css" rel="stylesheet" />

  <!-- Theme style -->
  <link href="~/assets/libs/AdminLTE/css/AdminLTE.min.css" rel="stylesheet" />

  <!-- AdminLTE Skins. Choose a skin from the css/skins
       folder instead of downloading all of them to reduce the load. -->
  <link href="~/assets/libs/AdminLTE/css/skins/_all-skins.min.css" rel="stylesheet" />

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
  <!-- Google Font -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">

  @RenderSection("CSS", required: false)


  <!-- jQuery 3 -->
  <script src="~/assets/libs/jquery/jquery.min.js"></script>

  <script src="~/assets/js/common.js"></script>
  <script src="~/assets/js/ClientUsers/ClientsMaster.js"></script>
  <script src="~/assets/js/ClientUsers/ClientsComon.js"></script>

</head>
<body class="hold-transition skin-blue sidebar-mini fixed">

  @Html.Hidden("rdtClientsIndex", Url.Action("Index", "Clients"));
  @Html.Hidden("rdtClientsLogin", Url.Action("Login", "Clients"));
  @Html.Hidden("rdtClientsNewShipment", Url.Action("NewShipment", "Clients"));
  @Html.Hidden("rdtClientsPrintShipments", Url.Action("PrintShipments", "Clients"));
  @Html.Hidden("rdtClientsRequestDriver", Url.Action("RequestDriver", "Clients"));
  @Html.Hidden("rdtClientsContactUS", Url.Action("ContactUS", "Clients"));
  @Html.Hidden("rdtClientsUploadExcel", Url.Action("UploadExcel", "Clients"));

  <script type='text/javascript'>
    var ClientData = GetClientData();
    var AccessToken = ClientData.AccessToken;
    if (IsNullOrEmpty(AccessToken)) {
      window.location.href = "@Url.Action("Login", "Clients")";

    }

  </script>

  <div class="wrapper">
    <header class="main-header">

      <!-- Logo -->
      <a href="@Url.Action("Index", "Clients")" class="logo">
        <!-- mini logo for sidebar mini 50x50 pixels -->
        <span class="logo-mini"><b>MPS</b></span>
        <!-- logo for regular state and mobile devices -->
        <span class="logo-lg"><b>MPS Dispatch</b></span>
      </a>
      <!-- Header Navbar: style can be found in header.less -->
      <nav class="navbar navbar-static-top">
        <!-- Sidebar toggle button-->
        <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
          <span class="sr-only">Toggle navigation</span>
        </a>
        <div class="navbar-custom-menu">
          <ul class="nav navbar-nav">
            <!-- User Account: style can be found in dropdown.less -->
            <li class="dropdown user user-menu">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                <img src="~/assets/libs/AdminLTE/img/user2-160x160.jpg" class="user-image" alt="User Image">
                <span class="hidden-xs" id="username1"></span>
              </a>
              <ul class="dropdown-menu">
                <!-- User image -->
                <li class="user-header">
                  <img src="~/assets/libs/AdminLTE/img/user2-160x160.jpg" class="img-circle" alt="User Image">
                  <p>
                    <span class="hidden-xs" id="username2"></span>
                    -
                    <span class="hidden-xs" id="accountno"></span>
                    @*<small>Client since Nov. 2012</small>*@
                  </p>
                </li>
                <!-- Menu Body -->
                <!-- Menu Footer-->
                <li class="user-footer">
                  <div class="pull-left">
                    <a href="@Url.Action("ChangePassword", "Clients")" class="btn btn-default btn-flat">Change Password</a>
                  </div>
                  <div class="pull-right">
                    <a href="#" id="outBtn" class="btn btn-default btn-flat">Sign out</a>
                  </div>
                </li>
              </ul>
            </li>
          </ul>
        </div>
      </nav>
    </header>
    <!-- Left side column. contains the logo and sidebar -->
    <aside class="main-sidebar">
      <!-- sidebar: style can be found in sidebar.less -->
      <section class="sidebar">
        <!-- Sidebar user panel -->
        <div class="user-panel">
          <div class="pull-left image">
            <img src="~/assets/libs/AdminLTE/img/user2-160x160.jpg" class="img-circle" alt="User Image">
          </div>
          <div class="pull-left info">
            <p><span class="hidden-xs" id="username3"></span></p>
          </div>
        </div>
        <!-- sidebar menu: : style can be found in sidebar.less -->
        <ul class="sidebar-menu" data-widget="tree">
          <li class="header">MAIN NAVIGATION</li>


          <li>
            <a href="">
              <i class="fa fa-th"></i><span>Dashboard</span>
              <span class="pull-right-container"></span>
            </a>
          </li>


          <li><a href="@Url.Action("SchedulePickup", "Clients")"><i class="fa fa-circle-o"></i><span>Schedule Pickup</span></a></li>
          @*<li><a href="@Url.Action("PickupInquiry", "Clients")"><i class="fa fa-circle-o"></i><span>Pickup Inquiry</span></a></li>*@
          <li><a href="@Url.Action("NewShipment", "Clients")"><i class="fa fa-circle-o"></i><span>New Shipment</span></a></li>
          <li><a href="@Url.Action("PrintShipments", "Clients")"><i class="fa fa-circle-o"></i><span>Shipments Inquiry</span></a></li>
          <li><a href="@Url.Action("ContactUS", "Clients" )"><i class="fa fa-circle-o"></i><span>Contact US</span></a></li>
          <li><a href="@Url.Action("UploadExcel", "Clients")"><i class="fa fa-circle-o"></i><span>Upload Excel</span></a></li>

        </ul>
      </section>
      <!-- /.sidebar -->
    </aside>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
      @RenderBody()
    </div>
    <!-- /.content-wrapper -->
    <footer class="main-footer">
      <div class="pull-right hidden-xs">
        <b>Version</b> 2.4.0
      </div>
      <strong>
        <img style="width:32px;height:32px" src="~/assets/img/DSP_Logo.png" class="user-image" alt="User Image">
        Copyright &copy; 2017-2022 <a href="http://MpsEgypt.com">MPS Egypt</a>.
      </strong> All rights
      reserved.
    </footer>
  </div>


  <!-- ./wrapper -->
  <!-- jQuery UI 1.11.4 -->
  <script src="~/assets/libs/jquery-ui/jquery-ui.min.js"></script>
  <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
  <script>
    $.widget.bridge('uibutton', $.ui.button);
  </script>
  <!-- Bootstrap 3.3.7 -->
  <script src="~/assets/libs/bootstrap3/js/bootstrap.min.js"></script>
  <!-- Slimscroll -->
  <script src="~/assets/libs/Slimscroll/jquery.slimscroll.min.js"></script>
  <!-- AdminLTE App -->
  <script src="~/assets/libs/AdminLTE/js/adminlte.min.js"></script>


  <!-- Sweet Alert -->
  @*<script src="~/assets/libs/bootstrap-swal/sweetalert.min.js"></script>*@
<script src="~/assets/plugins/sweetalert2/sweetalert2.all.min.js"></script>

  <script src="~/assets/js/loadingoverlay/loadingoverlay.min.js"></script>

  @RenderSection("Scripts", required: false)


  <script>
    $(document).ready(function () {
      UpdateMasterData();

      $("#outBtn").off().on('click', function () {
        logout();
      });

    });
  </script>

</body>
</html>
